<Window x:Class="FoxyBrowser716.TabMoveWindowCard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        WindowStyle="None"
        AllowsTransparency="True"
        mc:Ignorable="d"
        Title="TabMoveWindowCard" 
        Height="34" Width="260"
        Background="Transparent"
        Topmost="True">
    <Border Name="RootPanel1" BorderBrush="#99FF9103" BorderThickness="2" Margin="0" Padding="0">
        <Grid Name="RootPanel2" Background="#CC1E1E2D" Margin="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Viewbox x:Name="TabIcon" Grid.Column="0" Margin="5" Width="20" Height="20" Stretch="Uniform"/>
            <Label x:Name="TitleLabel" Grid.Column="1" Height="30" Foreground="#ffff" Background="Transparent" VerticalAlignment="Center"/>
        </Grid>
    </Border>
</Window>
