<Window x:Class="FoxyBrowser716.TabMoveWindowCard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:FoxyBrowser716"
        mc:Ignorable="d"
        Title="TabMoveWindowCard" Height="30" Width="260"
        Background="#881E1E2D">
    <Grid Name="RootPanel" Background="Transparent">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Viewbox x:Name="TabIcon" Grid.Column="0" Margin="5" Width="20" Height="20" Stretch="Uniform"/>
        <Label x:Name="TitleLabel" Grid.Column="1" Height="30" Foreground="#ffff" Background="Transparent" VerticalAlignment="Center"/>
    </Grid>
</Window>
