<homeWidgets:IWidget x:Class="FoxyBrowser716.HomeWidgets.EditConfigWidget"
                 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                 xmlns:homeWidgets="clr-namespace:FoxyBrowser716.HomeWidgets"
                 xmlns:wpf="clr-namespace:Material.Icons.WPF;assembly=Material.Icons.WPF"
                 mc:Ignorable="d"
                 d:DesignHeight="100" d:DesignWidth="750"
                 Background="Transparent">
    <Button Width="{Binding RelativeSource={RelativeSource Self}, Path=ActualHeight}" 
            Height="Auto" Name="EditConfigButton" BorderThickness="0"
            Background="#002D2D30" Foreground="White"
            Content="{wpf:MaterialIconExt Kind=Edit}" >
    </Button>
</homeWidgets:IWidget>
